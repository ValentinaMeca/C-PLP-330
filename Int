#include <iostream> //used to include a file in our program and we need it to use count
int main() { //start a function
    std::cout << "Welcome to PLP 2\n"; //this prints a string 
    int a, b; //initializes variables
    int sum; //delcares sum as as int
    b = 5; //gives b a value
    a = 10; //gives a the value of 10
    sum = b + a; //to get sum add the ints
    std::cout << sum; //prints sum
    return 0; //this exits the program kind of like sc.close
}
